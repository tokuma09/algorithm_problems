	N = length(as)
	dp = falses(N, W)
	nums = zeros(Int, N, W) # Minimum number of as to achieve dp cells
	
	#First row
	if W â‰¥ as[1]
		dp[1, as[1]] = true
		nums[1, as[1]] = 1
	end
	# Second and later rows
	for n in 2:N, w in 1:W
		if dp[n-1, w]
			dp[n, w] = true
			nums[n, w] = nums[n - 1, w]
		elseif w == as[n]
			dp[n, w] = true
			nums[n, w] = 1
		elseif w > as[n] && dp[n - 1, w - as[n]] && nums[n - 1, w - as[n]] < k
			dp[n, w] = true
			nums[n, w] = nums[n - 1, w - as[n]] + 1
		end
	end
	
	return dp[N, W]
